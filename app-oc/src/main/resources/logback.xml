    <contextName>yunhuni-app-portal-api</contextName>
    <property name="APP_NAME" value="app-oc" />
    <property name="systemId" value="app.oc" />
    <include resource="logback-yunhuni.xml"/>
    <logger name="org.springframework.boot" level="INFO" additivity="true" />
